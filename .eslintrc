{
  "extends": [
    "a-standard", "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react", "babel"
  ],
  "rules": {
    "no-console": 0,
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 2,
    "react/prop-types": "off"
  }
}
