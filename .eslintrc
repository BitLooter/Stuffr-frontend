{
  "extends": [
    "a-standard", "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react", "babel"
  ],
  "rules": {
    "no-console": "off",
    "generator-star-spacing": "off",
    "babel/generator-star-spacing": "error",
    "react/prop-types": "off"
  }
}
